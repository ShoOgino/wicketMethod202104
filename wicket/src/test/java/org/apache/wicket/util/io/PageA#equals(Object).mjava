	/**
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	public boolean equals(Object obj)
	{
		if (obj instanceof PageA)
		{
			if (getNumericId() == ((PageA)obj).getNumericId())
			{
				if (page != null)
				{
					return page.equals(((PageA)obj).page);
				}
				return ((PageA)obj).page == null;
			}
		}
		return false;
	}

