    /**
     * @see wicket.util.diff.Delta#toRCSString(java.lang.StringBuffer, java.lang.String)
     */
    @Override
	public void toRCSString(StringBuffer s, String EOL)
    {
        s.append("a");
        s.append(original.anchor());
        s.append(" ");
        s.append(revised.size());
        s.append(EOL);
        revised.toString(s, "", EOL);
    }

