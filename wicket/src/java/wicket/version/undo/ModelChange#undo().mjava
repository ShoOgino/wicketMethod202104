	/**
	 * @see wicket.version.undo.Change#undo()
	 */
	@Override
	public void undo()
	{
		if (log.isDebugEnabled())
		{
			log.debug("UNDO MODEL CHANGE: setting original model " + originalModel + " to "
					+ component.getPath() + "@" + component.hashCode() + ")");
		}

		component.setModel(originalModel);
	}

