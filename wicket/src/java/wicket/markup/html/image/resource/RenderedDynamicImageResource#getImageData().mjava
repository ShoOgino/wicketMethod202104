  protected byte[] getImageData()
  {
    // get image data is always called in sync block
    byte[] data = null;
    if (imageData != null)
    {
      data = imageData.get();
    }
    if (data == null)
    {
      data = render();
      imageData = new SoftReference<byte[]>(data);
      setLastModifiedTime(Time.now());
    }
    return data;
  }

