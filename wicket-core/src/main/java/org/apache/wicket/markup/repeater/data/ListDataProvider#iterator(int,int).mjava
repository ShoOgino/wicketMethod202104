	/**
	 * @see IDataProvider#iterator(int, int)
	 */
	@Override
	public Iterator<? extends T> iterator(final int first, final int count)
	{
		List<T> list = getData();

		int toIndex = first + count;
		if (toIndex > list.size())
		{
			toIndex = list.size();
		}
		return list.subList(first, toIndex).listIterator();
	}

