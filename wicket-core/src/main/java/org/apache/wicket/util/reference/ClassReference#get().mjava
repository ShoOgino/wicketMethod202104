	/**
	 * @return the {@link Class} stored in this reference
	 */
	public Class<T> get()
	{
		Class<T> clazz = cache != null ? cache.get() : null;
		if (clazz == null)
		{
			clazz = WicketObjects.resolveClass(name);
			if (clazz == null)
			{
				throw new RuntimeException("Could not resolve class: " + name);
			}
			cache(clazz);
		}
		return clazz;
	}

