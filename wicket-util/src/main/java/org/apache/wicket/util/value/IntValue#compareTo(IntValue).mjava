	/**
	 * @param that
	 *            The object to compare with
	 * @return 0 if equal, -1 if less than or 1 if greater than
	 */
	@Override
	public final int compareTo(final IntValue that)
	{
		if (value < that.value)
		{
			return -1;
		}

		if (value > that.value)
		{
			return 1;
		}

		return 0;
	}

