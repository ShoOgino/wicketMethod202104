	/**
	 * Assert the tag body of the tag identified by wicket:id="wicketId" matches
	 * 'assertValue'
	 * 
	 * @param wicketId
	 * @param assertValue
	 * @throws Exception
	 */
	public void assertWicketIdTagText(final String wicketId, final String assertValue)
			throws Exception
	{
		assertXPath("//*[@wicket:id='" + wicketId + "']", assertValue);
	}

